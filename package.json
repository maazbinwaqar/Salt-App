{
  "name": "salt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios14": "react-native run-ios --simulator iphone 14 pro",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios:dev": "react-native run-ios --scheme 'development'",
    "ios:prod": "react-native run-ios --scheme 'production'",
    "ios:staging": "react-native run-ios --scheme 'staging'",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean-project": "react-native-clean-project",
    "report": "cd android && ./gradlew signingReport && cd ..",
    "bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "postversion": "cd android && ./gradlew assembleRelease",
    "gitPush": "git push -u origin DevOnePro",
    "build-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "postinstall": "react-native setup-ios-permissions && npx pod-install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/auth": "^18.7.3",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-bottom-tabs": "^6.2.19",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "apisauce": "^3.0.1",
    "lottie-ios": "3.4.1",
    "lottie-react-native": "^5.1.6",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.72.7",
    "react-native-animatable": "^1.4.0",
    "react-native-awesome-alerts": "^2.0.0",
    "react-native-blurhash": "^1.1.11",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "^6.2.3",
    "react-native-paper": "^5.11.4",
    "react-native-permissions": "^4.0.0",
    "react-native-reanimated": "^3.6.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.29.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux": "^5.0.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "rn-slide-button": "^1.0.3",
    "yup": "^1.3.2"
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency",
    "BluetoothPeripheral",
    "Calendars",
    "Camera",
    "Contacts",
    "FaceID",
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse",
    "MediaLibrary",
    "Microphone",
    "Motion",
    "Notifications",
    "PhotoLibrary",
    "PhotoLibraryAddOnly",
    "Reminders",
    "Siri",
    "SpeechRecognition",
    "StoreKit"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
